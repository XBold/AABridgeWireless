<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AABridgeWireless.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AABridgeWireless"
    Shell.FlyoutBehavior="Flyout"
    Title="AABridgeWireless">

    <ShellContent
        Title="Client"
        ContentTemplate="{DataTemplate local:ClientPage}"
        Route="client" />

    <ShellContent
        Title="Server"
        ContentTemplate="{DataTemplate local:ServerPage}"
        Route="server" />

</Shell>